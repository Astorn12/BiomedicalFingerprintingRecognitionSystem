            <Button.Background>
                <ImageBrush ImageSource="images/plus.png"/>
            </Button.Background>
        </Button>
        <Label Content="Gamma" Grid.Column="2" HorizontalAlignment="Left" Margin="161.6,10,0,0" VerticalAlignment="Top"/>
