            <Button.Background>
                <ImageBrush ImageSource="images/plus.png"/>
            </Button.Background>
        </Button>
        <Button Content="" Grid.Column="2" HorizontalAlignment="Left" Margin="214.6,133,0,0" VerticalAlignment="Top" Width="21" Height="21" RenderTransformOrigin="-0.905,0.657" Name="PsiPlus" Click="PsiPlusClick">
            <Button.Background>
                <ImageBrush ImageSource="images/plus.png"/>
            </Button.Background>
        </Button>
        <Button Content="" Grid.Column="2" HorizontalAlignment="Left" Margin="214.6,185,0,0" VerticalAlignment="Top" Width="21" Height="21" RenderTransformOrigin="-0.905,0.657" Name="SigmaPlus" Click="SigmaPlusClick">
            <Button.Background>
                <ImageBrush ImageSource="images/plus.png"/>
            </Button.Background>
        </Button>
