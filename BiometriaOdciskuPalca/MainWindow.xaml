        <Label Content="Psi" Grid.Column="2" HorizontalAlignment="Left" Margin="163.6,102,0,0" VerticalAlignment="Top"/>
        <Label Content="Sigma" Grid.Column="2" HorizontalAlignment="Left" Margin="161.6,154,0,0" VerticalAlignment="Top"/>
        <Label Content="Theta" Grid.Column="2" HorizontalAlignment="Left" Margin="158.6,206,0,0" VerticalAlignment="Top"/>
        <Button Content="Zastosuj" Grid.Column="2" HorizontalAlignment="Left" Margin="141.6,274,0,0" VerticalAlignment="Top" Width="94" Height="31"/>
        <Button Content="GaborFilter" Grid.Column="2" HorizontalAlignment="Left" Margin="141.6,329,0,0" VerticalAlignment="Top" Width="94" Height="31" Click="GaborFilterClick"/>
        <Button Content="MedianFilter" Grid.Column="2" HorizontalAlignment="Left" Margin="141.6,374,0,0" VerticalAlignment="Top" Width="94" Height="31" Click="MedianFilter"/>
        <Button Content="Normalization" Grid.Column="2" HorizontalAlignment="Left" Margin="141.6,421,0,0" VerticalAlignment="Top" Width="94" Height="31" Click="Normalization"/>
        <Button Content="Random" Grid.Column="2" HorizontalAlignment="Left" Margin="141.6,469,0,0" VerticalAlignment="Top" Width="94" Height="36" Click="Random"/>
        <Button Content="OK" Grid.Column="2" HorizontalAlignment="Left" Margin="141.6,516,0,0" VerticalAlignment="Top" Width="94" Height="30" Click="OK"/>
        <Button Content="Histogram" Grid.Column="2" HorizontalAlignment="Left" Margin="141.6,562,0,0" VerticalAlignment="Top" Width="94" Height="28" Click="Histogram"/>
        <Button Content="Binaryzation" Grid.Column="2" HorizontalAlignment="Left" Margin="141.6,604,0,0" VerticalAlignment="Top" Width="94" Height="32" Click="Binaryzation"/>






