        <Button Content="Start&#xD;&#xA;" Grid.Column="2" HorizontalAlignment="Left" Margin="26.6,30,0,0" VerticalAlignment="Top" Width="75" Height="21" Click="Start1"/>
        <TextBox Grid.Column="2" HorizontalAlignment="Left" Height="23" Margin="131.6,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="30" RenderTransformOrigin="-0.567,-0.622" Name="GammaInsert"/>
