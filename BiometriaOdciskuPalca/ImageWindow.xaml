<Window x:Class="BiometriaOdciskuPalca.ImageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BiometriaOdciskuPalca"
        mc:Ignorable="d"
        Title="ImageWindow" Height="500" Width="500" >
    <ScrollViewer MouseDoubleClick="ScrollViewer_MouseDoubleClick">
        <Grid >
            <Image x:Name="obrazek" Stretch="UniformToFill" Margin="0,-10,9.8,0.4" />
        </Grid>
    </ScrollViewer>
</Window>
